[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Hendrik Eeckhaut (heeckhau)
[access "refs/for/refs/*"]
	read = group user/Hendrik Eeckhaut (heeckhau)
	push = group user/Hendrik Eeckhaut (heeckhau)
	label-Code-Review = -2..+2 group user/Hendrik Eeckhaut (heeckhau)
	label-Verified = -1..+1 group user/Hendrik Eeckhaut (heeckhau)
	rebase = group user/Hendrik Eeckhaut (heeckhau)
	removeReviewer = group user/Hendrik Eeckhaut (heeckhau)
	submit = group user/Hendrik Eeckhaut (heeckhau)
[access "refs/heads/*"]
	read = group user/Hendrik Eeckhaut (heeckhau)
	create = group user/Hendrik Eeckhaut (heeckhau)
	push = +force group user/Hendrik Eeckhaut (heeckhau)
	pushMerge = group user/Hendrik Eeckhaut (heeckhau)
[access "refs/tags/*"]
	push = +force group user/Hendrik Eeckhaut (heeckhau)
	pushTag = group user/Hendrik Eeckhaut (heeckhau)
	pushSignedTag = group user/Hendrik Eeckhaut (heeckhau)
